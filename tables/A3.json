{
    "tables": [
        {
            "name": "A.3 Форма сбора сведений об узлах очистки газа газораспределительных станций",
            "sub_forms": [
                {
                    "name": "A.3.1 Сведения об фильтрующих элементах узла очистки газа",
                    "fields": {
                        "Тип": {
                            "type": "list",
                            "options": ["ТипA", "ТипB", "ТипC", "ТипD"]
                        },
                        "Марка": {
                            "type": "string",
                            "pattern": "Марка{i}"
                        },
                        "Объем V (м3)": {
                            "type": "float",
                            "min": 10.0,
                            "max": 500.0,
                            "distribution": "uniform",
                            "round": 1
                        },
                        "Пропускная способность Q (тыс. м3/час)": {
                            "type": "float",
                            "min": 1.0,
                            "max": 100.0,
                            "distribution": "gaussian",
                            "round": 1
                        },
                        "Dу (МПа)": {
                            "type": "float",
                            "min": 0.5,
                            "max": 15.0,
                            "distribution": "uniform",
                            "round": 2
                        },
                        "Дата ввода в эксплуатацию": {
                            "type": "date",
                            "min": "2000-01-01",
                            "max": "2024-12-31"
                        },
                        "Дата следующего тех. освидетельствования": {
                            "type": "date",
                            "min": "2024-01-01",
                            "max": "2026-12-31"
                        },
                        "Регистрационный номер ГГТН РФ": {
                            "type": "registration_number",
                            "pattern": "GGTN-{0000}-{0000}"
                        },
                        "Завод-изготовитель": {
                            "type": "string",
                            "pattern": "Завод{i}"
                        },
                        "Выявленные дефекты": {
                            "type": "list",
                            "options": ["ДефектA", "ДефектB", "ДефектC", "ДефектD"]
                        },
                        "Примечание": {
                            "type": "string",
                            "pattern": "Примечание {i}"
                        }
                    }
                },
                {
                    "name": "A.3.2 Сведения о ЗРА узла очистки газа",
                    "fields": {
                        "Тип ЗРА": {
                            "type": "list",
                            "options": ["Тип1", "Тип2", "Тип3", "Тип4"]
                        },
                        "Привод": {
                            "type": "string",
                            "pattern": "Привод{i}"
                        },
                        "Год ввода в эксплуатацию": {
                            "type": "integer",
                            "min": 1990,
                            "max": 2024,
                            "distribution": "uniform",
                            "round": 0
                        },
                        "Dy (мм)": {
                            "type": "float",
                            "min": 100.0,
                            "max": 1000.0,
                            "distribution": "uniform",
                            "round": 1
                        },
                        "Давление, Py (МПа)": {
                            "type": "float",
                            "min": 0.5,
                            "max": 15.0,
                            "distribution": "gaussian",
                            "round": 2
                        },
                        "Завод изготовитель": {
                            "type": "string",
                            "pattern": "Завод{i}"
                        },
                        "Недопустимые дефекты в соответствии с СТО Газпром 2-4.1-406-2009": {
                            "type": "list",
                            "options": ["Дефект1", "Дефект2", "Дефект3", "Дефект4"]
                        },
                        "Ремонтопригодных дефектов": {
                            "type": "integer",
                            "min": 0,
                            "max": 10,
                            "distribution": "uniform",
                            "round": 0
                        },
                        "Примечание": {
                            "type": "string",
                            "pattern": "Примечание {i}"
                        }
                    }
                }
            ]
        }
    ]
}
